{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "name": "contact"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "regex": "\\d+",
      "required": true
    },
    "message": {
      "type": "text",
      "required": true
    }
  }
}
